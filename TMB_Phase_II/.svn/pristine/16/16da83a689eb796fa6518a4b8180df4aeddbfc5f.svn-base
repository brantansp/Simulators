<?xml version="1.0" encoding="utf-8"?>
<TableRow
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="wrap_content" android:background="#fcfbf2">
    <RelativeLayout android:id="@+id/relativeLayout1" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
        <LinearLayout android:layout_width="wrap_content" android:orientation="vertical" android:layout_height="wrap_content" android:id="@+id/linearLayout1" android:layout_alignParentLeft="true" android:gravity="center_horizontal|left|right">
            <TextView android:layout_height="wrap_content" android:gravity="center_horizontal" android:layout_width="wrap_content" android:id="@+id/rowlineleft1" android:text="TextView"></TextView>
            <TextView android:layout_height="wrap_content" android:gravity="center_horizontal" android:layout_width="wrap_content" android:id="@+id/rowlineleft2" android:text="TextView"></TextView>
        </LinearLayout>
        <LinearLayout android:layout_width="wrap_content" android:orientation="vertical" android:layout_height="wrap_content" android:id="@+id/LinearLayout01" android:layout_alignParentRight="true" android:gravity="center_horizontal|right">
            <TextView android:layout_height="wrap_content" android:gravity="center_horizontal" android:layout_width="wrap_content" android:id="@+id/rowlineright1" android:text="TextView"></TextView>
            <TextView android:layout_height="wrap_content" android:gravity="center_horizontal" android:layout_width="wrap_content" android:id="@+id/rowlineright2" android:text="TextView"></TextView>
        </LinearLayout>
    </RelativeLayout>
  
    
</TableRow>
